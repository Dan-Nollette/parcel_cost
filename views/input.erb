<h1>Dans Ship it Now!</h1>

<form action="/output">
  <div class="form-group">
    <label for="length">Please enter the length of your package.</label>
    <input id="length" name="length" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="width">Please enter the width of your package.</label>
    <input id="width" name="width" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="height">Please enter the height of your package.</label>
    <input id="height" name="height" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="weight">Please enter the weight of your package.</label>
    <input id="weight" name="weight" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">Submit</button>
</form>
